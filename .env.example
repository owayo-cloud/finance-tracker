# Example .env for CI/local testing (do NOT store secrets here)
# Update values as needed for local development. Keep secrets out of version control.

# Domain
DOMAIN=localhost
FRONTEND_HOST=http://localhost:5173
ENVIRONMENT=local

# Project
STACK_NAME=fsf-ci-test
PROJECT_NAME="Full Stack FastAPI Project"

# Postgres (used by tests)
POSTGRES_SERVER=db
POSTGRES_PORT=5432
POSTGRES_DB=app_test
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# App
SECRET_KEY=example-secret-key
FIRST_SUPERUSER=admin@example.com
FIRST_SUPERUSER_PASSWORD=changeme

# Images (used during build / deploy in CI)
DOCKER_IMAGE_BACKEND=backend
DOCKER_IMAGE_FRONTEND=frontend
TAG=latest
